{
  "version": 1,
  "serviceConfiguration": {
    "apiName": "SimpleAuthentication",
    "serviceName": "AppSync",
    "defaultAuthType": {
      "mode": "API_KEY",
      "keyDescription": "This is an api key",
      "expirationTime": 30
    },
    "additionalAuthTypes": [
      {
        "mode": "AWS_IAM"
      }
    ],
    "conflictResolution": {
      "defaultResolutionStrategy": {
        "type": "AUTOMERGE"
      }
    }
  }
}